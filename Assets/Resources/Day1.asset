  phrases:
  - speaker:
      name: "\u0414\u0436\u0435\u0439\u043C\u0441"
    text: "\u041F\u043E\u0447\u0442\u0438 \u043D\u0430 \u043C\u0435\u0441\u0442\u0435."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0414\u043E\u0431\u0440\u043E\u0434\u0443\u0448\u043D\u043E\u0435 \u043B\u0438\u0446\u043E
      \u0414\u0436\u0435\u0439\u043C\u0441\u0430, \u0432 \u0431\u043E\u043B\u044C\u0448\u0438\u043D\u0441\u0442\u0432\u0435
      \u0441\u043B\u0443\u0447\u0430\u0435\u0432 \u043E\u0431\u043E\u0437\u043D\u0430\u0447\u0430\u044E\u0449\u0435\u0435
      \u043B\u0451\u0433\u043A\u043E\u0435 \u0431\u0435\u0437\u0440\u0430\u0437\u043B\u0438\u0447\u0438\u0435.
      \u042D\u0442\u043E \u0442\u043E, \u043A \u0447\u0435\u043C\u0443 \u0435\u0433\u043E
      \u043A\u043E\u043C\u043F\u0430\u043D\u044C\u043E\u043D\u044B \u043F\u0440\u0438\u0432\u044B\u043A\u043B\u0438
      \u0438 \u0442\u043E, \u0447\u0435\u043C \u043E\u043D \xAB\u0440\u0430\u0434\u043E\u0432\u0430\u043B\xBB
      \u0438\u0445 \u0432\u0441\u044E \u0434\u043E\u0440\u043E\u0433\u0443."
  - speaker:
      name: "\u0410\u0440\u0442\u044D\u043D"
    text: "\u041F\u0440\u0438\u044F\u0442\u043D\u043E \u0437\u043D\u0430\u0442\u044C,
      \u0447\u0442\u043E \u0442\u044B \u0440\u0430\u0437\u0434\u0435\u043B\u044F\u0435\u0448\u044C
      \u043D\u0430\u0448 \u044D\u043D\u0442\u0443\u0437\u0438\u0430\u0437\u043C \u043F\u043E
      \u043F\u043E\u0432\u043E\u0434\u0443 \u044D\u0442\u043E\u0433\u043E \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u043E\u0433\u043E
      \u043F\u0440\u0438\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u044F."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u041C\u043D\u043E\u0433\u043E \u0447\u0430\u0449\u0435 \u0443\u043B\u044B\u0431\u0447\u0438\u0432\u044B\u0439
      \u0438 \u043A\u0443\u0434\u0430 \u0431\u043E\u043B\u0435\u0435 \u0431\u0435\u0437\u0437\u0430\u0431\u043E\u0442\u043D\u044B\u0439
      \u0410\u0440\u0442\u044D\u043D. \u0411\u043E\u043B\u044C\u0448\u0430\u044F
      \u0443\u0434\u0430\u0447\u0430 \u0434\u043B\u044F \u043D\u0435\u0433\u043E
      \u0441\u0430\u043C\u043E\u0433\u043E, \u0435\u0433\u043E \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0435
      \u043D\u0430\u043B\u0438\u0447\u0438\u0435 \u043F\u0440\u0438 \u0441\u0435\u0431\u0435."
  - speaker:
      name: "\u0417\u0430\u0440\u0438"
    text: "\u0422\u0435\u043C \u0431\u043E\u043B\u0435\u0435 \u0443\u0447\u0438\u0442\u044B\u0432\u0430\u044F,
      \u0447\u0442\u043E \u0442\u044B \u0432\u0441\u0451 \u0438 \u0437\u0430\u0442\u0435\u044F\u043B\u2026"
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0418 \u0417\u0430\u0440\u0438 \u2013 \u0445\u0440\u0443\u043F\u043A\u0430\u044F,
      \u043D\u0438\u0437\u0435\u043D\u044C\u043A\u0430\u044F \u0438 \u0441\u043A\u0440\u043E\u043C\u043D\u0430\u044F\u2026"
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u041A\u0430\u043A\u0438\u0445 \u0442\u043E\u043B\u044C\u043A\u043E \u043B\u044E\u0434\u0435\u0439
      \u043D\u0435 \u0441\u043E\u0431\u0438\u0440\u0430\u0435\u0442 \u0432\u043C\u0435\u0441\u0442\u0435
      \u0432\u0440\u0435\u043C\u044F. \u0422\u0430\u043A \u0438 \u044D\u0442\u0430
      \u0440\u0430\u0437\u043D\u043E\u0448\u0451\u0440\u0441\u0442\u043D\u0430\u044F
      \u0442\u0440\u043E\u0438\u0446\u0430, \u0440\u043E\u0441\u0448\u0430\u044F
      \u0441 \u043C\u0430\u043B\u043E\u043B\u0435\u0442\u043D\u0438\u0445 \u043B\u0435\u0442
      \u0432\u043C\u0435\u0441\u0442\u0435, \u0431\u044B\u043B\u0430 \u043F\u0440\u0438\u043C\u0435\u0440\u043E\u043C
      \u043A\u0440\u0435\u043F\u043A\u043E\u0439 \u0438 \u043D\u0435\u0440\u0443\u0448\u0438\u043C\u043E\u0439
      \u0434\u0440\u0443\u0436\u0431\u044B. "
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u041D\u0443 \u043A\u0430\u043A \u0442\u0440\u043E\u0438\u0446\u0430\u2026 
      \u0414\u0436\u0435\u0439\u043C\u0441 \u0438 \u0410\u0440\u0442\u044D\u043D
      \u0434\u0440\u0443\u0437\u044C\u044F \u0441 \u0434\u0435\u0442\u0441\u0442\u0432\u0430.
      \u0420\u0430\u0437\u043D\u0438\u0446\u0430 \u0432 \u0432\u043E\u0437\u0440\u0430\u0441\u0442\u0435
      \u043D\u0435 \u0441\u043E\u0441\u0442\u0430\u0432\u043B\u044F\u0435\u0442 \u0438
      \u0433\u043E\u0434\u0430. \u0418 \u0441\u0440\u0435\u0434\u0438 \u043F\u0440\u043E\u0447\u0438\u0445
      \u043C\u0430\u043B\u044C\u0447\u0438\u0448\u0435\u043A \u043D\u0430 \u0444\u0435\u0440\u043C\u0435,
      \u0442\u043E\u043B\u044C\u043A\u043E \u044D\u0442\u0438 \u0434\u0432\u043E\u0435
      \u043F\u043E\u043F\u0430\u0434\u0430\u043B\u0438 \u0432 \u0441\u0430\u043C\u044B\u0435
      \u043A\u0430\u0432\u0435\u0440\u0437\u043D\u044B\u0435 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0412\u0441\u0435\u0433\u0434\u0430 \u0432\u043C\u0435\u0441\u0442\u0435,
      \u0432\u0441\u0435\u0433\u0434\u0430 \u0434\u0440\u0443\u0433 \u0437\u0430
      \u0434\u0440\u0443\u0433\u0430. \u0410\u0440\u0442\u044D\u043D \u2013 \u0431\u043E\u043B\u044C\u0448\u043E\u0439
      \u043B\u044E\u0431\u0438\u0442\u0435\u043B\u044C \u043F\u043E\u0431\u0435\u0434\u0430\u043A\u0443\u0440\u0438\u0442\u044C.
      \u0418 \u0414\u0436\u0435\u0439\u043C\u0441 \u2013 \u043A\u0442\u043E \u0432\u0441\u0435\u0433\u0434\u0430
      \u0431\u044B\u043B \u0440\u044F\u0434\u043E\u043C, \u0447\u0442\u043E\u0431\u044B
      \u0432\u044B\u0440\u0443\u0447\u0438\u0442\u044C \u0438 \u0442\u0430\u043A\u0436\u0435,
      \u0447\u0442\u043E\u0431\u044B \u043E\u0442\u0447\u0438\u0442\u0430\u0442\u044C."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0421 \u043F\u043E\u0434\u0440\u043E\u0441\u0442\u043A\u043E\u0432\u043E\u0433\u043E
      \u0432\u043E\u0437\u0440\u0430\u0441\u0442\u0430, \u043C\u0430\u043B\u044C\u0447\u0438\u0448\u043A\u0438
      \u043E\u043A\u0430\u0437\u0430\u043B\u0438\u0441\u044C \u0432 \u043F\u0430\u0440\u0435,
      \u0440\u0430\u0431\u043E\u0442\u0430\u044F \u043D\u0430 \u0437\u0435\u043C\u043B\u0435.
      \u042D\u0442\u043E \u0431\u044B\u043B\u0430 \u0441\u0430\u043C\u0430\u044F
      \u043F\u0440\u043E\u0434\u0443\u043A\u0442\u0438\u0432\u043D\u0430\u044F \u0433\u0440\u0443\u043F\u043F\u0430.
      \u041D\u043E \u0432\u0441\u0451 \u0436\u0435, \u0410\u0440\u0442\u044D\u043D
      \u043D\u0438\u043A\u043E\u0433\u0434\u0430 \u043D\u0435 \u043B\u0438\u0448\u0430\u043B
      \u0441\u0435\u0431\u044F \u0443\u0434\u043E\u0432\u043E\u043B\u044C\u0441\u0442\u0432\u0438\u044F
      \u043F\u043E\u0441\u0430\u0447\u043A\u043E\u0432\u0430\u0442\u044C."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0427\u0435\u043C \u0432\u0437\u0440\u043E\u0441\u043B\u0435\u0435 \u0438
      \u043A\u0440\u0443\u043F\u043D\u0435\u0435 \u043E\u043D \u0441\u0442\u0430\u043D\u043E\u0432\u0438\u043B\u0441\u044F,
      \u0442\u0435\u043C \u0441\u0438\u043B\u044C\u043D\u0435\u0435 \u043E\u043D
      \u043B\u044E\u0431\u0438\u043B \u0432\u0437\u0434\u0440\u0435\u043C\u043D\u0443\u0442\u044C,
      \u043F\u043E\u0435\u0441\u0442\u044C, \u043F\u043E\u0432\u0430\u043B\u044F\u0442\u044C\u0441\u044F.
      \u0418 \u0432 \u0442\u043E \u0436\u0435 \u0432\u0440\u0435\u043C\u044F \u043F\u043E\u0438\u0433\u0440\u0430\u0442\u044C,
      \u043F\u043E\u0434\u0440\u0430\u0442\u044C\u0441\u044F. "
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0414\u0436\u0435\u0439\u043C\u0441 \u0434\u0440\u0443\u0433\u043E\u0439.
      \u041E\u043D \u0441\u0434\u0435\u0440\u0436\u0430\u043D\u043D\u0435\u0435,
      \u0441\u043F\u043E\u043A\u043E\u0439\u043D\u0435\u0435 \u0438 \u0441\u043E\u0441\u0440\u0435\u0434\u043E\u0442\u043E\u0447\u0435\u043D\u043D\u0435\u0435.
      \u0418 \u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043D\u043D\u0435\u0435.
      \u041B\u044E\u0431\u0443\u044E \u0440\u0430\u0431\u043E\u0442\u0443 \u043E\u043D
      \u0432\u044B\u043F\u043E\u043B\u043D\u044F\u0435\u0442 \u0441\u043A\u0440\u0443\u043F\u0443\u043B\u0451\u0437\u043D\u043E.
      \u0418 \u0432\u0441\u0451 \u044D\u0442\u043E \u043F\u0440\u0438 \u0442\u043E\u043C,
      \u0447\u0442\u043E \u043D\u0435 \u0431\u044B\u043B\u043E \u043D\u0438\u043A\u043E\u0433\u043E,
      \u043A\u0442\u043E \u0431\u044B \u0442\u0430\u043A \u0436\u0435 \u0441\u0438\u043B\u044C\u043D\u043E
      \u043D\u0435 \u043B\u044E\u0431\u0438\u043B \u0431\u044B\u0442\u044C \u0444\u0435\u0440\u043C\u0435\u0440\u043E\u043C."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0421\u043C\u044B\u0441\u043B \u0442\u0430\u043A\u043E\u0439 \u0436\u0438\u0437\u043D\u0438
      \u0443\u0434\u0440\u0443\u0447\u0430\u0435\u0442 \u0414\u0436\u0435\u0439\u043C\u0441\u0430.
      \u0418 \u0437\u0430\u0441\u0442\u0430\u0432\u043B\u044F\u044E\u0442 \u0435\u0449\u0451
      \u0431\u043E\u043B\u044C\u0448\u0435 \u0437\u043B\u0438\u0442\u044C\u0441\u044F
      \u0440\u0430\u0437\u0431\u043E\u0439\u043D\u0438\u043A\u0438, \u043F\u0440\u043E\u043C\u044B\u0448\u043B\u044F\u044E\u0449\u0438\u0435
      \u043D\u0430 \u044E\u0433\u0435 \u0424\u0435\u043B\u0435\u0441\u0442\u0438\u043D\u044B.
      \u0412\u0440\u044F\u0434 \u043B\u0438 \u043E\u043D \u0441\u043C\u043E\u0436\u0435\u0442
      \u043F\u0440\u0438\u043F\u043E\u043C\u043D\u0438\u0442\u044C, \u0447\u0442\u043E
      \u0438\u043C\u0435\u043D\u043D\u043E \u0437\u0430\u0441\u0442\u0430\u0432\u0438\u043B\u043E
      \u0435\u0433\u043E \u0442\u0430\u043A \u0434\u0443\u043C\u0430\u0442\u044C,
      \u043D\u043E \u0414\u0436\u0435\u0439\u043C\u0441 \u0432\u0441\u0435\u043C
      \u0441\u0432\u043E\u0438\u043C \u043D\u0443\u0442\u0440\u043E\u043C \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u0435\u0442
      \u2013 \u0441\u0442\u0435\u0437\u044F \u0413\u0435\u0440\u043E\u044F \u0435\u0433\u043E
      \u043F\u0443\u0442\u044C."
  - speaker:
      name: "\u0414\u0436\u0435\u0439\u043C\u0441"
    text: "\u042F \u0432\u0441\u0451 \u0435\u0449\u0451 \u043F\u043E\u043B\u043E\u043D
      \u0440\u0435\u0448\u0438\u043C\u043E\u0441\u0442\u0438, \u0435\u0441\u043B\u0438
      \u0432\u044B \u043E\u0431 \u044D\u0442\u043E\u043C."
  - speaker:
      name: "\u0410\u0440\u0442\u044D\u043D"
    text: "\u041A\u0430\u043A \u0438 \u043C\u044B. \u041C\u044B \u0432\u0435\u0434\u044C
      \u0441 \u0442\u043E\u0431\u043E\u0439. \u0417\u0434\u0435\u0441\u044C \u0438
      \u0441\u0435\u0439\u0447\u0430\u0441."
  - speaker:
      name: "\u0417\u0430\u0440\u0438"
    text: "\u041D\u0443, \u044F \u0432\u0441\u0451 \u0435\u0449\u0451 \u0441\u0447\u0438\u0442\u0430\u044E
      \u0432\u0441\u0451 \u044D\u0442\u043E, \u043E\u043F\u0430\u0441\u043D\u043E\u0439
      \u0437\u0430\u0442\u0435\u0435\u0439. \u0415\u0441\u043B\u0438 \u0432\u044B
      \u0445\u043E\u0442\u0438\u0442\u0435 \u0437\u043D\u0430\u0442\u044C, \u0447\u0442\u043E
      \u044F \u0434\u0443\u043C\u0430\u044E."
  - speaker:
      name: "\u0410\u0440\u0442\u044D\u043D"
    text: "\u041A\u043E\u043D\u0435\u0447\u043D\u043E, \u043C\u0438\u043B\u0430\u044F!
      \u0422\u0430\u043A \u0447\u0442\u043E, \u0414\u0436\u0435\u0439\u043C\u0441,
      \u043A\u0430\u043A\u043E\u0432 \u043F\u043B\u0430\u043D?"
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0410\u0440\u0442\u044D\u043D \u0438 \u0417\u0430\u0440\u0438 \u0434\u0430\u0432\u043D\u043E
      \u0432\u043C\u0435\u0441\u0442\u0435. \u041E\u043D\u0430 \u0440\u043E\u0441\u043B\u0430
      \u043D\u0430 \u0444\u0435\u0440\u043C\u0435 \u043F\u043E \u0441\u043E\u0441\u0435\u0434\u0441\u0442\u0432\u0443.
      \u0418 \u043A\u043E\u043D\u0435\u0447\u043D\u043E, \u043D\u0435 \u0431\u044B\u043B\u0430
      \u0442\u0430\u043A\u043E\u0439 \u0436\u0435 \u0434\u0440\u0443\u0436\u043D\u043E\u0439
      \u0438 \u0442\u0435\u0441\u043D\u043E\u0439 \u0447\u0430\u0441\u0442\u044C\u044E
      \u0432\u0441\u0435\u0445 \u0442\u0440\u043E\u0438\u0445."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u041F\u043E\u0441\u043B\u0435 \u0437\u043D\u0430\u043A\u043E\u043C\u0441\u0442\u0432\u0430
      \u0441 \u0410\u0440\u0442\u044D\u043D\u043E\u043C \u043D\u0430 \u043E\u0434\u043D\u043E\u043C
      \u0438\u0437 \u043F\u0440\u0430\u0437\u0434\u043D\u0438\u043A\u043E\u0432 \u0432
      \u0447\u0435\u0441\u0442\u044C \u0443\u0440\u043E\u0436\u0430\u044F, \u043E\u043D\u0438
      \u0434\u043E\u0441\u0442\u0430\u0442\u043E\u0447\u043D\u043E \u0434\u043E\u043B\u0433\u043E
      \u043F\u0440\u0438\u0442\u0438\u0440\u0430\u043B\u0438\u0441\u044C \u0434\u0440\u0443\u0433
      \u043A \u0434\u0440\u0443\u0433\u0443. \u0427\u0442\u043E \u043D\u0435 \u0443\u0434\u0438\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u043E,
      \u0443\u0447\u0438\u0442\u044B\u0432\u0430\u044F \u0440\u0430\u0437\u043D\u0438\u0446\u0443
      \u043C\u0435\u0436\u0434\u0443 \u043D\u0438\u043C\u0438 \u043A\u0430\u043A
      \u0432 \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043A\u043E\u043C, \u0442\u0430\u043A
      \u0438 \u0432 \u0434\u0443\u0448\u0435\u0432\u043D\u043E\u043C \u043F\u043B\u0430\u043D\u0435. "
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0411\u043E\u043B\u0435\u0435 \u0442\u043E\u0433\u043E, \u0442\u0438\u0445\u0430\u044F
      \u0438 \u043D\u0435\u0443\u0432\u0435\u0440\u0435\u043D\u043D\u0430\u044F \u0417\u0430\u0440\u0438
      \u043A\u0443\u0434\u0430 \u0431\u044B\u0441\u0442\u0440\u0435\u0435 \u0441\u0434\u0440\u0443\u0436\u0438\u043B\u0430\u0441\u044C
      \u0441 \u0414\u0436\u0435\u0439\u043C\u0441\u043E\u043C. \u0418 \u0445\u043E\u0442\u044C
      \u043E\u043D\u0438 \u0431\u043E\u043B\u044C\u0448\u0435 \u043F\u043E\u0434\u0445\u043E\u0434\u0438\u043B\u0438
      \u0434\u0440\u0443\u0433 \u0434\u0440\u0443\u0433\u0443, \u0438\u0441\u043A\u0440
      \u043C\u0435\u0436\u0434\u0443 \u043D\u0438\u043C\u0438 \u043D\u0435 \u043F\u0440\u043E\u0431\u0435\u0433\u0430\u043B\u043E."
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0427\u0442\u043E \u043E\u043F\u0440\u043E\u0432\u0435\u0440\u0433\u043B\u043E
      \u0438 \u0434\u043E\u043A\u0430\u0437\u0430\u043B\u043E \u0441\u0440\u0430\u0437\u0443
      \u0434\u0432\u0435 \u0438\u0441\u0442\u0438\u043D\u044B: \u0434\u0440\u0443\u0436\u0431\u0430
      \u043C\u0435\u0436\u0434\u0443 \u043C\u0443\u0436\u0447\u0438\u043D\u043E\u0439
      \u0438 \u0436\u0435\u043D\u0449\u0438\u043D\u043E\u0439 \u043D\u0435 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u0430
      \u0438, \u043F\u0440\u043E\u0442\u0438\u0432\u043E\u043F\u043E\u043B\u043E\u0436\u043D\u043E\u0441\u0442\u0438
      \u043F\u0440\u0438\u0442\u044F\u0433\u0438\u0432\u0430\u044E\u0442\u0441\u044F. "
  - speaker:
      name: "\u0410\u0432\u0442\u043E\u0440"
    text: "\u0410\u0440\u0442\u044D\u043D \u0438 \u0417\u0430\u0440\u0438 \u043F\u0440\u0438\u0432\u043B\u0435\u043A\u0430\u043B\u0438
      \u0434\u0440\u0443\u0433 \u0434\u0440\u0443\u0433\u0430 \u0441 \u043F\u0435\u0440\u0432\u043E\u0439
      \u0432\u0441\u0442\u0440\u0435\u0447\u0438. \u041E\u043D\u0438 \u043F\u0440\u043E\u0441\u0442\u043E
      \u0434\u043E\u043B\u0433\u043E \u0440\u0435\u0448\u0430\u043B\u0438\u0441\u044C
      \u043F\u043E\u0439\u0442\u0438 \u043D\u0430 \u043F\u0435\u0440\u0432\u044B\u0435
      \u0448\u0430\u0433\u0438."
  - speaker:
      name: "\u0414\u0436\u0435\u0439\u043C\u0441"
    text: "\u0412\u0441\u0451 \u043F\u0440\u043E\u0441\u0442\u043E. \u0418\u0434\u0451\u043C
      \u0432 \u0434\u0435\u0440\u0435\u0432\u043D\u044E, \u0437\u0430\u043A\u0443\u043F\u0430\u0435\u043C
      \u043F\u0440\u0438\u043F\u0430\u0441\u044B \u0438 \u0430\u043C\u0443\u043D\u0438\u0446\u0438\u044E."
